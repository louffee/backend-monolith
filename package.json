    "db:generate": "yarn run database:generate",
    "db:studio": "yarn run database:studio",
    "db:migrate": "yarn run database:migrate",
    "build": "rimraf build && npx esbuild --outfile=build/main.js --bundle src/main.ts --platform=node",
